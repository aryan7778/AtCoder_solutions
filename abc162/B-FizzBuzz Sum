#include <bits/stdc++.h>
#define fastIO ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
using namespace std;

int main() {
    fastIO
    long long int  n,sum=0;
    cin >> n;
    for(int i=1; i<=n; i++){
        if(i%3==0 || i%5==0){
            continue;
        }
        sum += i;
    }
    cout<<sum<<endl;
  return 0;
}

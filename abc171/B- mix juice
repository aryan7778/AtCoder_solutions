#include<bits/stdc++.h>
#define fastIO ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

using namespace std;

int main(){
	fastIO
  int n,k;
  cin>>n>>k;
  int arr[n],ans=0;
  for(int i=0;i<n;i++){
  	cin>>arr[i];
  }
  sort(arr,arr+n);
  for(int i=0;i<k;i++){
  	ans+=arr[i];
  }
  cout<<ans<<endl;
  return 0;
}

#include<bits/stdc++.h>
#include<string.h>
#define fastIO ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

using namespace std;

int main(){
	fastIO
	string str1,str2;
	cin>>str1>>str2;
	int count=0;
	for(int i=0;i<str1.length();i++){
		if(str1[i]!=str2[i]){
			count++;
		}
	}
	cout<<count<<endl;
	
	return 0;
}

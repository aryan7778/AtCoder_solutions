#include <bits/stdc++.h>
#define fastIO ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

using namespace std;

int main(){
  
  fastIO
  int n;
  cin>>n;
  int a=n%10;
  string str;
  if(a==3)
    str="bon";
  else if(a==0||a==1||a==6||a==8)
    str="pon";
  else
    str="hon";
  
  cout<<str<<endl;  
  
return 0;
}
